{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "es6": true,
        "mocha": true
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "comma-dangle": [
            "error",
            "never"
        ]
    }
}